<h1>All Kitchens</h1>

<ul>
  <% @kitchens.each do |kitchen| %>
  <li>
    <p><%= link_to kitchen.name, user_kitchen_path(kitchen.user, kitchen) %>, <%= kitchen.user.first_name%></p>
    <p><%= link_to "delete", admin_kitchen_path(kitchen), method: :delete, data: {confirm: 'Are you sure you want to delete your kitchen?'}%></p>
    <p> <%= kitchen.address%>, <%= kitchen.city%>, <%= kitchen.state%>, <%= kitchen.zipcode%></p>
  </li>
  <% end %>
</ul>
