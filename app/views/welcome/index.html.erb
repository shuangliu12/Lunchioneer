<%= form_tag search_path, method: :get do %>
  <div class="form-input">
    <%= text_field_tag :search, params[:q], placeholder: "enter your city" %>
    <%= date_select :date_search, params[:date_search] %>
  </div>

  <div class="form-submit">
    <%= submit_tag "Search" %>
  </div>
<% end %>

